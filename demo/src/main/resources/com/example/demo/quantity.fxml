<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="242.0" prefWidth="423.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.QuantityController">
   <children>
      <ImageView fitHeight="340.0" fitWidth="478.0" layoutX="-11.0" layoutY="-23.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@image/image2.jpg" />
         </image>
      </ImageView>
      <Label layoutX="120.0" layoutY="145.0" text="Enter Quantity" textFill="#f5f2f2">
         <font>
            <Font size="13.0" />
         </font>
      </Label>
      <TextField fx:id="txtQuantity" layoutX="205.0" layoutY="135.0" prefHeight="26.0" prefWidth="169.0" style="-fx-background-color: transparent; -fx-border-color: white; -fx-border-width: 0px 0px 1px 0px;" />
      <Button fx:id="btnCancel" layoutX="297.0" layoutY="256.0" mnemonicParsing="false" onAction="#cancelQuantity" style="-fx-background-color: white; -fx-background-radius: 10;" text="Cancel" textFill="BROWN" />
      <Button fx:id="btnDone" layoutX="138.0" layoutY="256.0" mnemonicParsing="false" onAction="#setQuantity" prefHeight="25.0" prefWidth="52.0" style="-fx-background-color: white; -fx-background-radius: 10;" text="Done" textFill="BROWN">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
